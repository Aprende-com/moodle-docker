cd ~
if [ -f ~/moodle-docker/.env ]; then
    export $(cat ~/moodle-docker/.env | xargs | envsubst)
else
    echo "Cannot find .env file."
fi
sudo cp ~/moodle-docker/config.docker-template.php $MOODLE_DOCKER_WWWROOT/config.php
sudo cp ~/moodle-docker/mdl-init.sh $MOODLE_DOCKER_WWWROOT/
cd ~/moodle-docker
bin/moodle-docker-compose up -d
